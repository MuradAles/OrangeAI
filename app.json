{
  "expo": {
    "name": "Avo-AI",
    "slug": "MessageAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/avo-ai.jpg",
    "scheme": "avoai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#f5d9a0",
        "foregroundImage": "./assets/images/avo-ai.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.murad.messageai",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/avo-ai.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/avo-ai.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f5d9a0",
          "dark": {
            "backgroundColor": "#3d5a3d"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/avo-ai.jpg",
          "color": "#6FA86F"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "EXPO_PUBLIC_FIREBASE_API_KEY": "$EXPO_PUBLIC_FIREBASE_API_KEY",
      "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "$EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN",
      "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "$EXPO_PUBLIC_FIREBASE_PROJECT_ID",
      "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "$EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET",
      "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "$EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID",
      "EXPO_PUBLIC_FIREBASE_APP_ID": "$EXPO_PUBLIC_FIREBASE_APP_ID",
      "router": {},
      "eas": {
        "projectId": "abdece3f-a5c7-4a4b-92bb-471bc2ba5d9b"
      }
    },
    "owner": "monsiermyrad",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/abdece3f-a5c7-4a4b-92bb-471bc2ba5d9b"
    }
  }
}
